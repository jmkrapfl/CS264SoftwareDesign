@startuml
class Mouth {
    + <<override>> name : string <<get>>
    + <<override>> xCoord : int <<get>> <<set>>
    + <<override>> yCoord : int <<get>> <<set>>
    + <<override>> style : char <<get>> <<set>>
    + <<override>> rotate : int <<get>> <<set>>
